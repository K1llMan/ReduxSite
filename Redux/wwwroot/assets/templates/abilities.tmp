<template id="ability">
    <div class="ability-row flow-right">
        <div class="ability-caption"> {{caption}} </div>
        <div class="abilities-count flow-down">
            <span class="pick">{{pickCount}}</span>
            <span class="winrate">{{winrateCount}}</span>
        </div>
        <div class="flow-down bars">
            <div class="pick-bar">
                <div class="pick" style="width: {{pickPerc}}%;"></div>
                <div class="ban" style="width: {{banPerc}}%;"></div>
            </div>
            <div class="winrate-bar" style="width: {{pickPerc}}%;">
                <div class="loss" style="width: {{lossPerc}}%;"></div>
                <div class="win" style="width: {{winPerc}}%;"></div>
            </div>
        </div>
    </div>
</template>

<template id="delete">
    <div class="fixed-action-btn horizontal" style="bottom: 25px; right: 24px;">
        <a class="btn-floating btn-large red tooltipped" data-position="top" data-delay="50" data-tooltip="Clear" id="removeSelectedMenu">
            <i class="large material-icons">delete</i>
        </a>
    </div>
</template>